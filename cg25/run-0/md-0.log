# Reset the timestep
undump          icrd
reset_timestep  0

# Timestep and Neighbour searching
timestep	1.0
neighbor        2.0 bin
neigh_modify	delay 0 every 1 check yes

# Fixes
fix 		shake spc shake 0.0001 10 0 b 1 a 1
  0 = # of size 2 clusters
  0 = # of size 3 clusters
  0 = # of size 4 clusters
  2832 = # of frozen angles
fix   		npt all npt temp 300.0 300.0 100.0 iso 1.0 1.0 10000.0

# Initial velocity
velocity	all create 300 432567 dist uniform

# Write out thermodynamic information
thermo_style    custom step temp press vol pe ke evdwl ecoul epair
thermo	        250

# Write out trajectory
dump            cxtc all xtc 50 traj-0.xtc

# Write out velocity
#dump 		1 all custom 1 vel_npt.txt vx vy vz

# Checkpoint files
restart         5000 state_cpt-*.data

# Final run command
run             1500
